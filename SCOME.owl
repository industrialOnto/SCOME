    </owl:Class>
    


    <!-- http://www.semanticweb.org/indonto/ontologies/2014/0/SCOME#Base_Unit -->

    <owl:Class rdf:about="http://www.semanticweb.org/indonto/ontologies/2014/0/SCOME#Base_Unit">
