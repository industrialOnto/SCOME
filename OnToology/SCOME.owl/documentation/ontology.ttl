                                                                  owl:onClass :Base_Unit
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Derived_Unit ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :dimensionalSize ;
                                       owl:someValuesFrom xsd:float
                                     ] ;
