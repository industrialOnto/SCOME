###  http://www.semanticweb.org/indonto/ontologies/2014/0/SCOME#Value_Scale
:Value_Scale rdf:type owl:Class ;
             rdfs:subClassOf [ rdf:type owl:Class ;
                               owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                      owl:complementOf [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :maxValue ;
                                                                                         owl:someValuesFrom xsd:float
                                                                                       ]
                                                                    ]
                                                                    [ rdf:type owl:Class ;
                                                                      owl:complementOf [ rdf:type owl:Restriction ;
                                                                                         owl:onProperty :minValue ;
                                                                                         owl:someValuesFrom xsd:float
                                                                                       ]
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ]
                                             [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :maxValue ;
                                                                      owl:someValuesFrom xsd:float
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :minValue ;
                                                                      owl:someValuesFrom xsd:float
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ]
                                           )
                             ] ,
                             [ rdf:type owl:Class ;
                               owl:unionOf ( [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :hasScaleType ;
                                                                      owl:someValuesFrom :Numerical
                                                                    ]
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty :valueType ;
                                                                      owl:hasValue "Continuous"
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ]
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :valueType ;
                                               owl:hasValue "Discrete"
                                             ]
                                             [ rdf:type owl:Restriction ;
                                               owl:onProperty :valueType ;
                                               owl:hasValue "Linguistic"
                                             ]
                                           )
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasScaleType ;
                               owl:someValuesFrom :Scale_Type
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :valueType ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:oneOf [ rdf:type rdf:List ;
                                                                rdf:first "Continuous" ;
                                                                rdf:rest [ rdf:type rdf:List ;
                                                                           rdf:first "Discrete" ;
                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                      rdf:first "Linguistic" ;
                                                                                      rdf:rest rdf:nil
                                                                                    ]
                                                                         ]
                                                              ]
                                                  ]
                             ] ;
             rdfs:comment "Conjunto de posibles valores de una propiedad y las relaciones entre sí- las cuales se determinan por el tipo de escala asociado" .


