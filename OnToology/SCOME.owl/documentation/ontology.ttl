                                            owl:someValuesFrom <http://www.semanticweb.org/indonto/ontologies/2014/0/SCOPRO#SC_Entity>
                                          ] ;
                      rdfs:subClassOf [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                               owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :hasAllowedValues ;
                                                                               owl:someValuesFrom [ owl:intersectionOf ( :Allowed_Values
                                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                                           owl:onProperty :hasLowerBound ;
                                                                                                                           owl:someValuesFrom xsd:float
                                                                                                                         ]
                                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                                           owl:onProperty :hasUpperBound ;
                                                                                                                           owl:someValuesFrom xsd:float
                                                                                                                         ]
                                                                                                                       ) ;
                                                                                                    rdf:type owl:Class
                                                                                                  ]
                                                                             ]
                                                                             [ rdf:type owl:Restriction ;
                                                                               owl:onProperty :hasValueScale ;
                                                                               owl:someValuesFrom [ owl:intersectionOf ( :Value_Scale
                                                                                                                         [ rdf:type owl:Class ;
                                                                                                                           owl:complementOf [ rdf:type owl:Restriction ;
                                                                                                                                              owl:onProperty :valueType ;
                                                                                                                                              owl:hasValue "Continuous"
                                                                                                                                            ]
                                                                                                                         ]
                                                                                                                       ) ;
                                                                                                    rdf:type owl:Class
                                                                                                  ]
                                                                             ]
                                                                           )
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :hasValueScale ;
                                                               owl:someValuesFrom [ owl:intersectionOf ( :Value_Scale
                                                                                                         [ rdf:type owl:Restriction ;
                                                                                                           owl:onProperty :valueType ;
                                                                                                           owl:hasValue "Continuous"
                                                                                                         ]
                                                                                                       ) ;
                                                                                    rdf:type owl:Class
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasAssessmentMethod ;
                                        owl:someValuesFrom :Metric
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasValueScale ;
                                        owl:someValuesFrom :Value_Scale
